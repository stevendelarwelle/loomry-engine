
    
<%= simple_form_for @license  do |f| %>
 
    
    <%= f.input :software_id , :collection => current_user.software.select(:name, :id), label_method: :name, value_method: :id %>
    <%= f.input :expiration_date  %>
    <%= f.input :email  %>
    <%= f.input :is_active  %>
  
    <%= f.button :submit %>
  
<% end %>

